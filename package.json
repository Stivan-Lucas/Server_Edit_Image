{
  "name": "server",
  "version": "0.1.0",
  "description": "Edição rápida fácil e exportação em PDF, integração com OneDrive e OneDrive Bassness mais para frente!",
  "license": "MIT",
  "author": "Stivan Lucas C",
  "type": "module",
  "main": "dist/server.js",
  "homepage": "https://github.com/Stivan-Lucas/Server_Edit_Image#readme",
  "bugs": {
    "url": "https://github.com/Stivan-Lucas/Server_Edit_Image/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stivan-Lucas/Server_Edit_Image.git"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npx biome format --write",
      "npx biome lint --write"
    ]
  },
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "format": "biome format",
    "format:write": "biome format --write",
    "format:check": "biome format --check",
    "lint": "biome lint",
    "lint:write": "biome lint --write",
    "lint:check": "biome lint --check",
    "check": "biome check",
    "check:write": "biome check --write",
    "prepare": "husky"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.49.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/rate-limit": "^10.3.0",
    "fastify": "^5.5.0",
    "fastify-type-provider-zod": "^5.0.3",
    "pino": "^9.9.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@tsconfig/node-ts": "^23.6.1",
    "@tsconfig/node24": "^24.0.1",
    "@types/node": "^24.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "pino-pretty": "^13.1.1",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  }
}
